<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>io.github.jdiscordbots</groupId>
		<artifactId>jdiscordbots-parent</artifactId>
		<version>1.0.3</version>
	</parent>
	<groupId>io.github.danthe1st</groupId>
	<artifactId>yagpl</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>Yet Another Graphical Programming Language</name>
	<description>I try to create a graphical programming language</description>
	<dependencies>
		<dependency>
			<groupId>org.openjfx</groupId>
			<artifactId>javafx-controls</artifactId>
			<version>12.0.2</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<source>14</source>
					<target>14</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>